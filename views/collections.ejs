<style>
  
#logoul li a:hover {
  border-bottom: none !important;
  color:#9e9e9e !important;
}

#wraptitle {
  bottom: 2px !important;
  position: absolute;
  width: 100%;
}

.loader {  
  -webkit-animation: spin 5s ease infinite;
  -moz-animation: spin 5s ease infinite;
  -o-animation: spin 5s ease infinite;
  -ms-animation: spin 5s ease infinite;
  animation: spin 5s ease infinite;
  position: absolute;
  left: 0px;
  right: 0px;
  top: 0px;
  bottom: 0px;
  margin: auto;
  height: 70px;
  width: 70px;
}

.loader > div {
  width: 70px;
  height: 70px;
  position: absolute;
  top: 0px;
  left: 0px;
}

.loader > div > span {
  display: block;
  float: left;
  width: 35px;
  height: 35px;    
  -webkit-animation: loading 2s linear infinite;
  -moz-animation: loading 2s linear infinite;
  -o-animation: loading 2s linear infinite;
  -ms-animation: loading 2s linear infinite;
  animation: loading 2s linear infinite;
  opacity: 0.5;
}

.loader > div:first-child > span {
  background-color: #A7A7A7;
}

.loader > div.second {  
  -webkit-transform: rotate(25deg);
  -moz-transform: rotate(25deg);
  -o-transform: rotate(25deg);
  -ms-transform: rotate(25deg);
  transform: rotate(25deg);
}

.loader > div.second > span {
  background-color: white;
}

.loader > div.third {
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}

.loader > div.third > span {
  background-color: #9C9898;
}
  
#wrap3 {
  width: 100%;
  height: 100%;
  display: none;
}

#wrap {
  overflow: visible;
}

.close2 {
  height: 11px;
  opacity: 0.7;
  padding: 17px;
  position: absolute;
  right: -15px;
  top: -38px;
  width: 11px;
  cursor: pointer;
}

.close2::before {
  content: '';
  background: url(http://ssl.gstatic.com/ui/v1/dialog/close-x.png);
  position: absolute;
  height: 11px;
  width: 11px;
  right: 17px;
}

#col3{
display:none;
}

.menus {
opacity: 0.2;
}
#previous{
opacity:0.2;
}
#next{
opacity:0.2;
}

#spanses{
  display:none;
}

#spanses {
  font-size: 14px;
  font-weight: 100;
  margin: 20px auto;
  color: rgba(83, 83, 83, 0.94);
}

@-webkit-keyframes loading {
  0% {
    border-radius: 0px;
    -webkit-transform: scale(1);
  }

  50% {
    border-radius: 5px;
    -webkit-transform: scale(0.75);
  }

  100% {
    border-radius: 0px;
    -webkit-transform: scale(1);
  }
}

  @-moz-keyframes loading {
    0% {
      border-radius: 0px;
      -moz-transform: scale(1);
    }

  50% {
    border-radius: 5px;
    -moz-transform: scale(0.75);
  }

  100% {
    border-radius: 0px;
    -moz-transform: scale(1);
  }
}

  @-o-keyframes loading {
    0% {
      border-radius: 0px;
      -o-transform: scale(1);
    }

  50% {
    border-radius: 5px;
    -o-transform: scale(0.75);
  }

  100% {
    border-radius: 0px;
    -o-transform: scale(1);
  }
}


  @-ms-keyframes loading {
    0% {
      border-radius: 0px;
      -ms-transform: scale(1);
    }

  50% {
    border-radius: 5px;
    -ms-transform: scale(0.75);
  }

  100% {
    border-radius: 0px;
    -ms-transform: scale(1);
  }
}

@keyframes loading {
  0% {
    border-radius: 0px;
    transform: scale(1);
  }

  50% {
    border-radius: 5px;
    transform: scale(0.75);
  }

  100% {
    border-radius: 0px;
    transform: scale(1);
  }
}

@media (max-width: 43.75em){
  #wcontent{
    padding: 0 20px !important;
  }
  .article {
    padding: 0px 0px 0px 0px;
    line-height: 331px;
  }
  .article iframe{
    width: 100% !important;
    height: 80% !important;
  }
  .insites{
    display: none !important;
  }
  .close2 {
    right: -11px;
    top: -11px;
  }
  #spanses{
    display:block !important;
  }
}
</style>
